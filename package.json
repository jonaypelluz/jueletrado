    "scripts": {
        "start": "craco start",
        "build": "craco build",
        "test": "REACT_APP_APP_ENV=test jest",
        "test:watch": "REACT_APP_APP_ENV=test jest --watch",
        "test:coverage": "REACT_APP_APP_ENV=test jest --coverage",
        "lint": "eslint '{src,__tests__}/**/*.{js,jsx,ts,tsx}'",
        "lint:fix": "eslint --fix '{src,__tests__}/**/*.{js,jsx,ts,tsx}'",
        "format": "prettier --write '{src,__tests__,public,ops}/**/*.{js,jsx,ts,tsx,css,md,scss,html}' --config ./.prettierrc.js",
